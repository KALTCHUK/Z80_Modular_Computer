    )
    (circuit 
      (use_layer B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 700.0)
    )
    (circuit 
      (use_layer B.Cu)
    )
