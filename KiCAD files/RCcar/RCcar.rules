  )
  (via 
    "Via[0-1]_1000:400_um-kicad_default" "Via[0-1]_1000:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_1000:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_1000:400_um-kicad_default"
