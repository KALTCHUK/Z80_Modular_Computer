    "Net-(D1-Pad2)" "Net-(J1-Pad3)" "Net-(J2-Pad1)" "Net-(J3-Pad2)" "Net-(J4-Pad1)" "Net-(J4-Pad2)" "Net-(J4-Pad3)" "Net-(J4-Pad4)"
    "Net-(J4-Pad5)" "Net-(J4-Pad6)" "unconnected-(J5-Pad1)" "Net-(J5-Pad2)" /RX "unconnected-(J5-Pad6)" /MISO /SCK
    /MOSI /RESET /TX "Net-(Q1-Pad1)" "Net-(Q1-Pad2)" "Net-(Q2-Pad2)" /buzzer /light
    "Net-(R7-Pad1)" "unconnected-(U1-Pad21)" "unconnected-(U1-Pad23)" "unconnected-(U1-Pad24)" "unconnected-(U1-Pad25)" "unconnected-(U1-Pad26)" "unconnected-(U1-Pad27)" "unconnected-(U1-Pad28)"
    (clearance_class default)
    (via_rule default)
    (rule
